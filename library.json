{
  "name": "esp-dsp",
  "version": "0.0.0",
  "platforms": ["native", "espressif32"],
  "build": {
    "srcDir": "modules",
    "srcFilter": [
      "+<common/**/*.c>",
      "+<**/*_ansi.c>",
      "+<**/*_init_*.c>",
      "+<**/*_init.c>",
      "+<**/*_gen.c>",
      "-<common/misc/**>",
      "-<**/test/**>",
      "-<**/tests/**>",
      "-<examples/**>",
      "-<applications/**>",
      "-<test_app/**>",
      "-<**/*.S>",
      "-<**/*.s>",
      "-<**/*_ae32*>",
      "-<**/*_aes3*>",
      "-<**/*_p4*>",
      "-<**/*_rv*>"
    ],
    "includeDir": ".",
    "extraScript": "pio_extra.py"
  }
}
